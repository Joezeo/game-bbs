<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.joezeo.community.mapper.UserMapper">
    <insert id="insert" parameterType="User">
        insert into t_user
        (name, account_id, token, bio, gmt_create, gmt_modify)
        values
        (#{name},#{account_id},#{token},#{bio},#{gmt_create},#{gmt_modify})
    </insert>

    <select id="selectByToken" resultType="User">
        select * from t_user
        where token = #{token}
    </select>
</mapper>